<h2>List of boys</h2>

@{ 
    string[] boys = { "Raj", "rev", "reva", "revu" };
    List<string> girls = new List<string> { "rani", "rani2", "rani3" };

}

<ol>
    @for (int i = 0; i < boys.Length; i++)
    {
    <li>@boys[i]</li>
    }
    
</ol>
<h2>List of Girls</h2>
<ul>
    @foreach (string item in girls)
    {
        <li>@item</li>
    }

</ul>
